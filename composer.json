{
  "name": "victor-prdh/recaptcha-bundle",
  "description": "Use Google Recaptcha v2/v3 as form type with Symfony 6.4+, see v1.X for symfony 5.4",
  "type": "symfony-bundle",
  "require": {
    "php": ">=8.1.0",
    "google/recaptcha": "^1.3",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/form": "^6.4|^7.0",
    "symfony/translation": "^6.4|^7.0",
    "symfony/validator": "^6.4|^7.0",
    "symfony/twig-bundle": "^6.4|^7.0",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "autoload": {
    "psr-4": {
      "VictorPrdh\\RecaptchaBundle\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Victor Prudhomme",
      "email": "contact@victor-prdh.com"
    }
  ],
  "extra": {
    "symfony": {
      "allow-contrib": "true"
    }
  }
}
